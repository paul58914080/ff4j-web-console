{
  "name": "ff4j-web-console",
  "version": "0.0.0",
  "description": "The web console for FF4J",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/paul58914080/ff4j-web-console.git"
  },
  "keywords": [
    "ff4j",
    "feature toggling",
    "console",
    "feature"
  ],
  "author": "Paul WILLIAMS",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/paul58914080/ff4j-web-console/issues"
  },
  "homepage": "https://paul58914080.github.io/ff4j-web-console/",
  "private": false,
  "scripts": {
    "ng": "ng",
    "start": "run-p mock:server 'ng serve'",
    "start:ff4j-dashboards": "run-p mock:server 'ng serve ff4j-dashboards --port=4201'",
    "start:ff4j-features": "run-p mock:server 'ng serve ff4j-features --port=4202'",
    "start:ff4j-properties": "run-p mock:server 'ng serve ff4j-properties --port=4203'",
    "build": "ng build",
    "build:prod": "ng build --prod --output-hashing none",
    "build:features-element": "ng build ff4j-features --prod --output-hashing none && node projects/ff4j-features/build/ff4j-features-elements-builder.js",
    "build:properties-element": "ng build ff4j-properties --prod --output-hashing none && node projects/ff4j-properties/build/ff4j-properties-elements-builder.js",
    "build:dashboards-element": "ng build ff4j-dashboards --prod --output-hashing none && node projects/ff4j-dashboards/build/ff4j-dashboards-elements-builder.js",
    "test": "ng test --watch=false",
    "test:ff4j-dashboards": "ng test ff4j-dashboards --watch=false",
    "test:ff4j-features": "ng test ff4j-features --watch=false",
    "test:ff4j-properties": "ng test ff4j-properties --watch=false",
    "test:all": "run-p test test:ff4j-features test:ff4j-properties test:ff4j-dashboards",
    "e2e": "ng e2e",
    "mock:server": "json-server --watch mock/data.json",
    "lint": "ng lint",
    "lint:html": "htmlhint --config lint/.htmlhintrc src/app/**/*.html",
    "lint:styles": "stylelint --config lint/.stylelintrc.json src/app/**/*.scss",
    "lint:html:ff4j-dashboards": "htmlhint --config lint/.htmlhintrc projects/ff4j-dashboards/src/**/*.html",
    "lint:styles:ff4j-dashboards": "stylelint --config lint/.stylelintrc.json projects/ff4j-dashboards/**/*.scss",
    "lint:html:ff4j-features": "htmlhint --config lint/.htmlhintrc projects/ff4j-features/src/**/*.html",
    "lint:styles:ff4j-features": "stylelint --config lint/.stylelintrc.json projects/ff4j-features/**/*.scss",
    "lint:html:ff4j-properties": "htmlhint --config lint/.htmlhintrc projects/ff4j-properties/src/**/*.html",
    "lint:styles:ff4j-properties": "stylelint --config lint/.stylelintrc.json projects/ff4j-properties/**/*.scss",
    "lint:all": "run-p lint lint:html lint:styles lint:html:ff4j-features lint:styles:ff4j-features lint:html:ff4j-properties lint:styles:ff4j-properties lint:html:ff4j-dashboards lint:styles:ff4j-dashboards",
    "format:prettier": "pretty-quick --staged",
    "format:all": "run-s lint:all format:prettier"
  },
  "dependencies": {
    "@angular/animations": "~9.1.0",
    "@angular/common": "~9.1.0",
    "@angular/compiler": "~9.1.0",
    "@angular/core": "~9.1.0",
    "@angular/elements": "^9.0.7",
    "@angular/forms": "~9.1.0",
    "@angular/platform-browser": "~9.1.0",
    "@angular/platform-browser-dynamic": "~9.1.0",
    "@angular/router": "~9.1.0",
    "bootstrap": "^4.4.1",
    "document-register-element": "^1.7.2",
    "rxjs": "~6.5.5",
    "tslib": "^1.11.2",
    "zone.js": "~0.10.2"
  },
  "devDependencies": {
    "@angular-builders/custom-webpack": "^9.1.0",
    "@angular-devkit/build-angular": "~0.901.4",
    "@angular/cli": "~9.1.4",
    "@angular/compiler-cli": "~9.1.0",
    "@angular/language-service": "~9.1.4",
    "@types/jasmine": "~3.5.10",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "^13.13.5",
    "angular-cli-ghpages": "^0.6.2",
    "codelyzer": "^5.2.2",
    "concat": "^1.0.3",
    "del": "^5.1.0",
    "fs-extra": "^9.0.0",
    "htmlhint": "^0.11.0",
    "husky": "^4.2.5",
    "jasmine-core": "~3.5.0",
    "jasmine-spec-reporter": "~5.0.2",
    "json-server": "^0.16.1",
    "karma": "~5.0.4",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage-istanbul-reporter": "~3.0.0",
    "karma-jasmine": "~3.1.1",
    "karma-jasmine-html-reporter": "^1.5.3",
    "karma-parallel": "^0.3.1",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.0.5",
    "pretty-quick": "^2.0.1",
    "protractor": "~5.4.4",
    "stylelint": "^13.3.3",
    "stylelint-config-sass-guidelines": "^7.0.0",
    "stylelint-scss": "^3.17.1",
    "ts-node": "~8.10.1",
    "tslint": "~6.1.2",
    "typescript": "~3.8.3"
  },
  "resolutions": {
    "karma/optimist/minimist": "~1.2.5",
    "stylelint/postcss-sass/gonzales-pe/minimist": "~1.2.5"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run format:all",
      "pre-push": "npm run test:all"
    }
  }
}
